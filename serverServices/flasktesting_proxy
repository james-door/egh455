server {

listen 80;

server_name localhost;


index index.html;
location /
{
	root /var/www/flasktesting/frontend/build;
	try_files $uri /index.html;
}

location /images/ {
	alias /var/www/identifiedTargets/;
        autoindex on;
        access_log off; 
        expires 30d;  
        add_header Cache-Control "public";
    }



location /data {

include uwsgi_params;

uwsgi_pass unix:/tmp/flasktesting.sock;

}

}
